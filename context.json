{

  "@context": {
    
    "id": "@id",
    "type": "@type",

    "Composable": {
    "@id": "http://cooperation.org/credentials/v1#Composable",
      "@context": {
        "anchoredResource": {
           "@id": "http://cooperation.org/credentials/v1#anchoredResource"
        },
        "digestMultibase": {
          "@id": "http://cooperation.org/credentials/v1#digestMultibase"
        },
        "linkedClaim": {
          "@id": "http://cooperation.org/credentials/v1#linkedClaim",
          "@type": "http://cooperation.org/credentials/v1#LinkedClaim"
        }
      } 
    },

   "LinkedClaim": {
      "@id": "http://cooperation.org/credentials#LinkedClaim",
      "@context": {
        "id": "@id",
        "type": "@type",

        "lc": "http://cooperation.org/credentials",

        "claim": {
           "@id": "lc:claim"
        },
        "aspect": {
          "@id": "lc:aspect"
        },
        "source": {
          "@id": "lc:source",
          "@type": "lc:ClaimSource"
        },
        "object": {
          "@id": "lc:object"
        },
        "statement": {
          "@id": "lc:statement"
        },
        "confidence": {
          "@id": "lc:confidence"
        },
        "effectiveDate": {
          "@id": "lc:effectiveDate",
          "@type": "xsd:dateTime"
        }
      }
    },

    "ClaimSource": {
       "@id": "http://cooperation.org/credentials#claimSource",
       "@type": "@type",
       "@context": {
           "sourceId": {},
           "sourceType": {},
           "dateObserved": "xsd:dateTime"
       }
    },

    "Answerable": {
      "@id": "http://cooperation.org/credentials/v1#Answerable",
      "@type": "@type",
      "@context": {
        "respondAt": {
          "@id": "http://cooperation.org/credentials/v1#respondAt",
          "@type": "@id",
          "@container": ["@set", "@list"]
      } 
    }
  }
}
